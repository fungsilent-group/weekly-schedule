{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "Weekly Shcedule",
    "description": "Prettier weekly schedule from slack",
    "action": {
        "default_popup": "index.html",
        "default_title": "Weekly Shcedule"
    },
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    },
    "host_permissions":[
        "<all_urls>"
    ],
    "permissions": [
        "cookies"
    ]
}
